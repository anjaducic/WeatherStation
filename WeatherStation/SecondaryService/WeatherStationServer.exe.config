<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
<system.serviceModel>
		<services>
			<service name="WeatherStationServer.WeatherStationService">
				<host>
					<baseAddresses>
						<add baseAddress="net.tcp://localhost:5000/IWeatherStationService" />
					</baseAddresses>
				</host>
				<!-- Service Endpoints -->
				<endpoint address="" binding="netTcpBinding" contract="SharedLibrary.IWeatherStationService" />
			</service>
		</services>
	</system.serviceModel>
</configuration>